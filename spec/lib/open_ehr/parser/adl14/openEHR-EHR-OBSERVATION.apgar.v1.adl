    details = <
        ["en-us"] = <
            language = <[ISO_639-1::en-us]>
            purpose = <"Record Apgar index or assessment for newborn. The root time of the event series is always birth.">
            use = <"Allows recording of infant well-being at 1,2,5 and or 10 mintues after birth. The total may be recorded alone - if this is all that is available - but all five ordinal values should be completed in order that the total can be calculated. The total is the sum of the five ordinal values (min 0, max 10).">
            keywords = <"apgar", "index", "score", "newborn">
            misuse = <"Only partially completing the score and adding less than five values to give the total.">
        >
        ["en"] = <
            language = <[ISO_639-1::en]>
            purpose = <"Record Apgar index or assessment for newborn. The root time of the event series is always birth.">
            use = <"Allows recording of infant well-being at 1,2,5 and or 10 mintues after birth. The total may be recorded alone - if this is all that is available - but all five ordinal values should be completed in order that the total can be calculated. The total is the sum of the five ordinal values (min 0, max 10).">
            keywords = <"apgar", "newborn", "index", "score">
            misuse = <"Only partially completing the score and adding less than five values to give the total.">
        >
        ["de"] = <
            language = <[ISO_639-1::de]>
            purpose = <"Zur Dokumentation des Apgar Wertes oder Beurteilung f%Gï¿½%@r Neugeborene. Der zu Grunde liegende Zeitpunkt der Ereignisreihe ist immer die Geburt.">
            use = <"Erm%Gï¿½%@glicht die Dokumentation des Wohlergehens des S%Gï¿½%@uglings 1, 2, 5 und/ oder 10 Minuten nach der Geburt. Die Summe kann einzeln dokumentiert werden - falls nur diese verf%Gï¿½%@gbar ist - es sollten aber alle f%Gï¿½%@nf Zahlenwerte vervollst%Gï¿½%@ndigt werden, damit die Summe errechnet werden kann. Die Gesamtsumme ist die Summe der f%Gï¿½%@nf Zahlenwerte (minimal 0, maximal 10).">
            keywords = <"Apgar", "Neugeborenes", "Index", "Wert">
            misuse = <"Nur teilweise Vervollst%Gï¿½%@ndigung der Werte und Addieren von weniger als f%Gï¿½%@nf Werten zur Gesamtsumme.">
        >
    >
